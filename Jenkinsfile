pipeline {
  agent any
  stages {
    stage('Dev') {
      steps {
        echo 'I am in the dev stage'
      }
    }

    stage('Build') {
      steps {
        echo 'I am in the Build Stage '
      }
    }

    stage('Test') {
      steps {
        echo 'I am in the Test stage '
      }
    }

  }
}